{
  "name": "@peasy-store/core",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    "./*.sql": "./src/*.sql.ts",
    "./*.ts": "./src/*.ts",
    "./*": "./src/*.ts"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "db": "sst shell drizzle-kit",
    "db:exec": "sst shell ../scripts/src/psql",
    "db:reset": "sst shell ../scripts/src/db-reset"
  },
  "devDependencies": {
    "@neondatabase/serverless": "^0.9.3",
    "@types/luxon": "3.4.2"
  },
  "dependencies": {
    "@aws-sdk/client-rds-data": "3.577.0",
    "@aws-sdk/client-sesv2": "^3.577.0",
    "@aws-sdk/credential-providers": "3.577.0",
    "@hono/zod-openapi": "0.11.0",
    "drizzle-kit": "0.21.1",
    "drizzle-orm": "0.30.10",
    "hono": "4.0.1",
    "luxon": "3.4.4",
    "nanoid": "5.0.7",
    "remeda": "1.61.0",
    "stripe": "15.2.0",
    "zod": "3.23.8"
  }
}
